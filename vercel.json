{
  "buildCommand": "cd backend && npx prisma generate && npx prisma migrate deploy && cd ../frontend && npm run build",
  "outputDirectory": "frontend/dist",
  "framework": null,
  "rewrites": [
    { "source": "/clients/(.*)", "destination": "/api" },
    { "source": "/engine/(.*)", "destination": "/api" }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "backend/**"
    }
  }
}
