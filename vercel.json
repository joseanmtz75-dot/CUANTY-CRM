{
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": null,
  "rewrites": [
    { "source": "/clients", "destination": "/api" },
    { "source": "/clients/(.*)", "destination": "/api" },
    { "source": "/engine/(.*)", "destination": "/api" }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "backend/**"
    }
  }
}
